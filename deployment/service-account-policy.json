{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "s3:PutObject",
                "s3:GetObject",
                "s3:ListBucketMultipartUploads",
                "s3:AbortMultipartUpload",
                "s3:ListBucketVersions",
                "s3:CreateBucket",
                "s3:ListBucket",
                "s3:DeleteObject",
                "s3:GetBucketLocation",
                "s3:ListMultipartUploadParts"
            ],
            "Resource": [
                "arn:aws:s3:::dbaker-daskhubdev-users",
                "arn:aws:s3:::dbaker-daskhubdev-users/*",
                "arn:aws:s3:::dbaker-daskhubdev-shared",
                "arn:aws:s3:::dbaker-daskhubdev-shared/*",
                "arn:aws:s3:::dbaker-daskhubdev-curated",
                "arn:aws:s3:::dbaker-daskhubdev-curated/*"
            ]
        },
        {
            "Effect": "Allow",
            "Action": "s3:ListAllMyBuckets",
            "Resource": "*"
        }
    ]
}
